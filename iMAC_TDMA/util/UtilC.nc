}

// convert pdr to snr
async command int16_t Util.pdr2Snr(uint8_t pdr) {
	uint8_t i, size;
	
	i = pdr / 10;
	if (!(pdr % 10)) {
		if (i > 1)
			i--;
	}
	size = sizeof(pdr_2_snr_table) / sizeof(pdr_2_snr_table[0]);
	if (i >= size)
		i = size - 1;
	return ((int16_t)pdr_2_snr_table[i] * 128 / 10);
}
#endif

